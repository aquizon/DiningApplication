<% if flash[:warning] %>
    <div>
      <div>
        <%= "#{flash[:warning]}" %>
      </div>
    </div>
<% end %>
<h1 class="header">New Dining Hall </h1>

<%= form_with model: @dininghall, local: true do |dininghall_form| %>
<br> 

    <%= render 'form_template', dininghall_form: dininghall_form%>
    <br>
    <%= dininghall_form.submit "Create Dining Hall", :class=>"btn_sign"%>
<% end %>
<br>
<%= link_to "Back", dininghalls_path, :class=>"back" %>