<div><h1>Dining Halls</h1></div>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Hours</th>
            <th>Breakfast Hours</th>
            <th>Time</th>
        </tr>
    </thead>
    <tbody>
        <%@dininghalls.each do |dh|%>
        <tr>
            <td> <%= link_to "#{dh.name}", dininghall_path(dh.id)%> </td>
            <td> <%= dh.hours%> </td>
            <td> <%= dh.breakfast_hours%> </td>
            <td> <%= dh.time%> </td>
        </tr>
        <%end%>
    </tbody>
</table>
<%= link_to "New Dining Hall", new_dininghall_path%>
